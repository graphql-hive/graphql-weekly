{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "incremental": true,
    "skipLibCheck": true,
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
