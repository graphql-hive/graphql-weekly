{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "codegen"],
      "outputs": ["dist/**"]
    },
    "codegen": {
      "outputs": ["src/generated/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["codegen"]
    },
    "typecheck": {
      "dependsOn": ["codegen"]
    }
  }
}
