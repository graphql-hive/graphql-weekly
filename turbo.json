{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "codegen"],
      "outputs": ["dist/**"]
    },
    "deploy": {
      "dependsOn": ["build", "^deploy"],
      "passThroughEnv": [
        "CLOUDFLARE_API_TOKEN",
        "CLOUDFLARE_ACCOUNT_ID",
        "CLOUDFLARE_ZONE_ID"
      ]
    },
    "codegen": {
      "outputs": ["src/generated/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["codegen"]
    },
    "typecheck": {
      "dependsOn": ["codegen"]
    },
    "lint": {},
    "lint:fix": {}
  }
}
