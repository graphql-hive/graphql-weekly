{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "verbatimModuleSyntax": false
  },
  "include": [
    "src",
    ".astro/types.d.ts",
    "astro.config.mjs",
    "codegen.ts",
    "graphql-codegen.ts"
  ]
}
