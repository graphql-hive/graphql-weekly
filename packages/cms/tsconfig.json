{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "verbatimModuleSyntax": false
  },
  "include": [
    "src",
    "e2e",
    "scripts",
    ".astro/types.d.ts",
    "astro.config.mjs",
    "codegen.ts",
    "graphql-codegen.ts",
    "playwright.config.ts",
    "vite.config.ts"
  ]
}
