{
  "name": "graphqlweekly-api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build && yarn build-func",
    "build-func": "yarn generate && yarn lambda:build",
    "lambda:build": "tsc -p apollo/tsconfig.json",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only apollo/server/graphql",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@as-integrations/aws-lambda": "^1.1.0",
    "@prisma/client": "4.7.1",
    "axios": "^1.2.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.27.4",
    "graphql": "^16.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-scalars": "^1.20.1",
    "jsonwebtoken": "^8.5.1",
    "nexus": "1.3.0",
    "nexus-prisma": "^0.35.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jsonwebtoken": "^8.5.0",
    "prisma": "4.7.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
