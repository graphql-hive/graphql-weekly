{
  "name": "graphqlweekly-api",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "wrangler dev",
    "build": "bun run generate && tsc",
    "generate": "bun run generate:prisma && bun run generate:schema",
    "generate:prisma": "prisma generate",
    "generate:schema": "bun ./scripts/schema.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@netlify/functions": "^1.4.0",
    "@pothos/core": "^3.24.0",
    "@pothos/plugin-prisma": "^3.40.1",
    "@pothos/plugin-relay": "^3.33.1",
    "@pothos/plugin-scope-auth": "^3.17.0",
    "@prisma/client": "4.7.1",
    "axios": "^1.4.0",
    "dotenv": "^8.2.0",
    "graphql": "^16.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-scalars": "^1.20.1",
    "graphql-yoga": "^3.4.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^20.2.5",
    "prisma": "4.7.1",
    "typescript": "^5.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
