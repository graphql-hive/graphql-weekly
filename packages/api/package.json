{
  "name": "graphqlweekly-api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build && yarn build-func",
    "build-func": "yarn generate && yarn lambda:build",
    "lambda:build": "tsc -p apollo/tsconfig.json",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only apollo/server/graphql"
  },
  "dependencies": {
    "@nexus/schema": "0.18.0",
    "@prisma/client": "2.11.0",
    "apollo-server-lambda": "^2.19.0",
    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.27.4",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "nexus-prisma": "0.12.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.11.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jsonwebtoken": "^8.5.0",
    "apollo-server": "^2.19.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
